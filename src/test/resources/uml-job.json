{
  "description": "Create Java and Ember.js models from Gliffy",
  "name": "OopScopeMarketing-6",
  "parameters": {},
  "importSteps": [
    {
      "name": "Gliffy Import",
      "converter": "umlGliffyConverter",
      "sourceUri": "${uml.gliffyRootUri}/MarketingModel-6.gliffy",
      "destinationUri": "${uml.workingFolderUri}/MarketingModel-6.uml.json"
    }
  ],
  "exportSteps": [
    {
      "generator": "handlebarsCodeGenerator",
      "destinationUri": "${uml.targetProjectUri}/java",
      "definitions": [
        {
          "name": "Java Entities",
          "filter": "classes",
          "templateFile": "classTemplate.java"
        },
        {
          "name": "Java Interfaces",
          "filter": "interfaces",
          "templateFile": "interfaceTemplate.java"
        },
        {
          "name": "Java Enumerations",
          "filter": "enumerations",
          "templateFile": "enumerationTemplate.java"
        }
      ]
    },
    {
      "generator": "handlebarsCodeGenerator",
      "destinationUri": "${uml.targetProjectUri}/ts",
      "definitions": [
        {
          "name": "Ember classes",
          "filter": "classes",
          "templateFile": "classTemplate.ts"
        },
        {
          "name": "Ember Interfaces",
          "filter": "interfaces",
          "templateFile": "interfaceTemplate.ts"
        },
        {
          "name": "Ember Enumerations",
          "filter": "enumerations",
          "templateFile": "enumerationTemplate.ts"
        }
      ]
    }
  ]
}
