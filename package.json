{
  "name": "uml-tools",
  "displayName": "uml-tools",
  "description": "UML Conversion Tools for VSCode",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:uml-tools.welcome"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "uml-tools.welcome",
        "title": "uml-tools:Welcome"
      },
      {
        "command": "uml-tools.convert",
        "title": "uml-tools:Convert to UML-Json"
      },
      {
        "command": "uml-tools.generate",
        "title": "uml-tools:Generate Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.30.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "handlebars": "^4.7.7"
  }
}
